[Desktop Entry]
Name=chownat
Icon=utilities-terminal
Comment=Allows two peers behind two separate NATs with no port forwarding and no DMZ setup on their routers to directly communicate with each other
TryExec=/usr/bin/chownat
Exec=sh -c '/usr/local/bin/appHelper.sh /usr/bin/chownat -h' $SHELL
StartupNotify=true
Terminal=true
Type=Application
Categories=X-BlackArch-Tunnel;