[Desktop Entry]
Name=netbios-share-scanner
Icon=utilities-terminal
Comment=This tool could be used to check windows workstations and servers if they have accessible shared resources.
TryExec=/usr/bin/netbios-share-scanner
Exec=sh -c '/usr/local/bin/appHelper.sh /usr/bin/netbios-share-scanner -h' $SHELL
StartupNotify=true
Terminal=true
Type=Application
Categories=X-BlackArch-Scanner;